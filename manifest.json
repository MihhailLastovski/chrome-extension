{
  "manifest_version": 3,
  "name": "Radiance Crm",
  "version": "1.0",
  "permissions": [
    "identity",
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "downloads",
    "contextMenus"
  ],
  "oauth2": {
    "client_id": "241696042449-fmjki9t6rkm5hmtc6gk7ju1eg4t89dtc.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets"
    ]
  },
  "action": {
    "default_popup": "pages/popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [ "script/contentScript.js" ]
    }
  ],
  "background": {
    "service_worker": "script/background.js"
  },
  "oauth2": {
    "client_id": "59202187170-ek0v1hi22555f5vrpj8nf4468akskip3.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/drive", "https://www.googleapis.com/auth/spreadsheets"]
  }
}